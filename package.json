{
  "private": true,
  "version": "0.1.7",
  "name": "clickup-time-tracker",
  "productName": "Time Tracker",
  "description": "A tool for painlessly tracking time on Clickup tasks",
  "repository": {
    "type": "git",
    "url": "https://github.com/gwleuverink/clickup-time-tracker"
  },
  "author": {
    "name": "Willem Leuverink",
    "email": "willem@leuver.ink",
    "url": "https://leuver.ink"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "build:osx": "npm run electron:build -p never",
    "build:win": "npm run electron:build -p never -- --win --x64",
    "build:linux": "npm run electron:build -p never -- --linux",
    "build:all": "npm run build:osx && npm run build:win && npm run build:linux",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@heroicons/vue": "^1.0.5",
    "core-js": "^3.6.5",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.1",
    "naive-ui": "^2.23.2",
    "sass": "^1.49.7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "v-offline": "^3.1.0",
    "vue": "^3.0.0",
    "vue-cal": "^4.2.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.0.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "postcss": "^7",
    "sass-loader": "^10.2.1",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
